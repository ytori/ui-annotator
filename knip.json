{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    "apps/*": {
      "entry": ["src/main.tsx", "src/routes/**/*.tsx", "vite.config.ts"],
      "project": ["src/**/*.{ts,tsx}"],
      "ignore": ["src/routeTree.gen.ts"],
      "ignoreDependencies": [
        "postcss",
        "tailwindcss",
        "tw-animate-css",
        "@tanstack/react-router-devtools"
      ]
    },
    "packages/*": {}
  },
  "ignoreExportsUsedInFile": true
}
